{
  "name": "job-tracker",
  "version": "0.0.0",
  "engines": {
    "node": "^4.2.2",
    "npm": "^2.14.7"
  },
  "description": "Job Tracker App",
  "homepage": "",
  "author": {
    "name": "Bitovi",
    "email": "contact@bitovi.com",
    "url": "bitovi.com"
  },
  "scripts": {
    "test": "testee test/test.html --browsers firefox --reporter Spec",
    "document": "documentjs",
    "build": "node build",
    "install": "node build.js"
  },
  "main": "job-tracker/index.stache!done-autorender",
  "files": [
    "."
  ],
  "keywords": [],
  "system": {
    "mocha": {
      "timeout": 5000,
      "ui": "bdd"
    },
    "npmAlgorithm": "flat",
    "configDependencies": [
      "live-reload"
    ],
    "bundle": [
      "job-tracker/index.stache!done-autorender"
    ],
    "npmIgnore": [],
    "map": {
      "chai": "chai/chai"
    },
    "paths": {
      "auth0-lock": "vendor/auth0/lock-8.2.min.js"
    },
    "meta": {
      "pikaday": {
        "format": "cjs"
      },
      "auth0-lock": {
        "format": "global",
        "exports": "Auth0Lock"
      }
    }
  },
  "dependencies": {
    "bootstrap": "^3.3.6",
    "can": "^2.3.15",
    "can-connect": "^0.3.4",
    "can-fixture": "^0.1.1",
    "can-ssr": "^0.11.2",
    "done-autorender": "^0.6.0",
    "done-component": "^0.4.0",
    "done-css": "^1.1.15",
    "es6-math": "^1.0.0",
    "generator-donejs": "^0.3.5",
    "jquery": "2.1.4",
    "jquery-transport-xdr": "^1.0.9",
    "lodash": "^3.10.1",
    "moment": "^2.11.0",
    "pikaday": "^1.4.0",
    "steal": "^0.16.0",
    "steal-socket.io": "^1.0.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "donejs-cli": "^0.5.8",
    "donejs-deploy": "^0.3.1",
    "funcunit": "^3.0.0",
    "mocha": "^3.0.2",
    "steal-mocha": "0.0.3",
    "steal-tools": "^0.16.0",
    "testee": "^0.2.2"
  }
}
